{
  "name": "react-universal-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel-node tools/run build",
    "start": "babel-node tools/run start",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "prettier": "find . -name \"*.js\" | grep -v -f .prettierignore | xargs prettier --tab-width 4 --single-quote --trailing-comma all --write",
    "prettier:tools": "find ./tools -name \"*.js\" | xargs prettier --tab-width 4 --single-quote --trailing-comma es5 --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "./*.js": [
      "prettier --tab-width 4 --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "tools/**/*.js": [
      "prettier --tab-width 4 --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --tab-width 4 --single-quote --trailing-comma all --write",
      "git add"
    ],
    "server/**/*.js": [
      "prettier --tab-width 4 --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-define": "1.3.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "css-loader": "0.28.4",
    "del": "3.0.0",
    "dotenv": "4.0.0",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "flow-bin": "0.49.1",
    "friendly-errors-webpack-plugin": "1.6.1",
    "fs-extra": "3.0.1",
    "gzip-size": "3.0.0",
    "hard-source-webpack-plugin": "0.4.6",
    "husky": "0.14.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "lint-staged": "4.0.0",
    "node-object-hash": "1.3.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.6",
    "prettier": "1.5.2",
    "pretty-bytes": "4.0.2",
    "react-addons-test-utils": "15.6.0",
    "react-hot-loader": "next",
    "react-test-renderer": "15.6.1",
    "redis-server": "1.1.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-middleware-multi-compiler": "1.0.0",
    "webpack-hot-middleware": "2.18.0",
    "webpack-hot-server-middleware": "0.1.0",
    "webpack-node-externals": "1.6.0",
    "write-file-webpack-plugin": "4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-universal-component": "1.4.1",
    "source-map-support": "0.4.15",
    "webpack-flush-chunks": "1.0.10",
    "winston": "2.3.1"
  }
}
